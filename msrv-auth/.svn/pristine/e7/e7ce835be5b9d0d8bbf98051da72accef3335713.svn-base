group=monitor
hotMap=hotMap
version=0.0.1

server.port=8089
spring.application.name=msrv-auth

logging.config=classpath:log4j2-local.xml

remote.url.outer.youge=http://192.168.1.105:8091/webServer
path.outer.youge.user-login=/userLogin/login

#\u7981\u7528http\u57FA\u672C\u534F\u8BAE\u8BA4\u8BC1
security.basic.enabled=false

#\u4EE3\u7406\u6E38\u9E3D\u540E\u53F0\u8BF7\u6C42\u8DEF\u5F84
