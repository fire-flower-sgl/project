package com.mhtech.platform.msrv.pojo;

import java.io.Serializable;
import java.util.Date;


public class ServerNotifyVO implements Serializable{
	private static final long serialVersionUID = 1L;
	
	

    private int size;
    private Date time;
    
	public ServerNotifyVO(int size, Date time) {
		super();
		this.size = size;
		this.time = time;
	}


	public int getSize() {
		return size;
	}


	public void setSize(int size) {
		this.size = size;
	}


	public Date getTime() {
		return time;
	}


	public void setTime(Date time) {
		this.time = time;
	}


	public static long getSerialversionuid() {
		return serialVersionUID;
	}


	public ServerNotifyVO() {
		super();
	}


	@Override
	public String toString() {
		return "ServerNotifyVO [size=" + size + ", time=" + time + "]";
	}
	
    
	
}
