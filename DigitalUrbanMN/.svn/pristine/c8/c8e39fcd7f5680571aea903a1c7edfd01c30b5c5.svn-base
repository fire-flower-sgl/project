package com.mhtech.platform.msrv.pojo;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

import java.io.Serializable;

@ApiModel("告警分类占比信息")
public class AlertRateVO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2473024501927162685L;

	@ApiModelProperty("分类名称")
	private String alertType;
	
	@ApiModelProperty("分类比例")
	private double alertRate;

	public String getAlertType() {
		return alertType;
	}

	public void setAlertType(String alertType) {
		this.alertType = alertType;
	}

	public double getAlertRate() {
		return alertRate;
	}

	public void setAlertRate(double alertRate) {
		this.alertRate = alertRate;
	}
}
