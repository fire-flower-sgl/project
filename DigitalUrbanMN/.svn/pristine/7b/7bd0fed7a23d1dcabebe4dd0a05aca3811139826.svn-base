package com.mhtech.platform.msrv.pojo;

import java.io.Serializable;

/**
 * 特殊权限关联表
 * @author Administrator
 *
 */
public class SpePowerDTO implements Serializable{

	private static final long serialVersionUID = 1L;
	
	
	private String userCode;

	private String speNum;
	
	public String getSpeNum() {
		return speNum;
	}

	public void setSpeNum(String speNum) {
		this.speNum = speNum;
	}

	public SpePowerDTO(String userCode, String speNum) {
		super();
		this.userCode = userCode;
		this.speNum = speNum;
	}

	public String getUserCode() {
		return userCode;
	}

	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}

	public SpePowerDTO() {
		super();
	}

	public SpePowerDTO(String userCode) {
		super();
		this.userCode = userCode;
	}
	
}
