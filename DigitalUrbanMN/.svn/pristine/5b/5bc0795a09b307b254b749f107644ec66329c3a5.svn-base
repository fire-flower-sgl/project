package com.tools.modules.entity;

/**
 * @ClassName SysChart
 * @Description TODO 图表配置模块
 * @Author admin
 * @Date 2019/8/28 13:54
 * @Version 1.0
 */
public class SysChart {

	private String chartId;   //图表编码
	
	private String chartName; //图表名称
	
	private String template;  //图表模板

	private String isuse;        //是否可用 
	
	private String type;         //图表类型
	public String getChartName() {
		return chartName;
	}

	public void setChartName(String chartName) {
		this.chartName = chartName;
	}

	public String getTemplate() {
		return template;
	}

	public void setTemplate(String template) {
		this.template = template;
	}
	
	
	public String getChartId() {
		return chartId;
	}

	public void setChartId(String chartId) {
		this.chartId = chartId;
	}


	

	public String getIsuse() {
		return isuse;
	}

	public void setIsuse(String isuse) {
		this.isuse = isuse;
	}

	public SysChart() {
		super();
	}

	
	
	
	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
	public String toString() {
		return "SysChart [chartId=" + chartId + ", chartName=" + chartName + ", template=" + template + ",isuse="+isuse+"]";
	}
	//重写toString是对对象在打印输出时候的一种格式化。这样做符合业务逻辑，显示结果人性化。

	public SysChart(String chartId, String chartName, String template, String isuse) {
		super();
		this.chartId = chartId;
		this.chartName = chartName;
		this.template = template;
		this.isuse = isuse;
	}

	public SysChart(String chartName, String template, String isuse) {
		super();
		this.chartName = chartName;
		this.template = template;
		this.isuse = isuse;
	}
	
	
}
