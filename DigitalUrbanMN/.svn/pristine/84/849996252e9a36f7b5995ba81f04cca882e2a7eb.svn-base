package com.mobile.model;

import java.io.Serializable;

/**
 * @ClassName CustomerMobiles
 * @Description TODO 客户对应手机号
 * @Author admini
 * @Date 2019/8/28 17:03
 * @Version 1.0
 */
public class CustomerMobiles implements Serializable{
    /**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	/**
     * 主键
     **/
    String id;
    /**
     * 客户编码
     **/
    String customerId;
    /**
     * 手机号
     **/
    String mobileno;
    /**
     * 状态
     **/
    String state;
    /**
     * 使用次数
     **/
    String callnumber;
    /**
     * 说明
     **/
    String remark;

    /**
     * 创建时间
     **/
    String ctime;
    /**
     * 姓名
     */
    String name;
    /**
            * 身份证号码
     */
    String idcard;
    //运营商
    String operators;
    public CustomerMobiles() {
    }


    public CustomerMobiles(String id, String customerId, String mobileno, String state, String callnumber,
			String remark, String ctime, String name, String idcard) {
		super();
		this.id = id;
		this.customerId = customerId;
		this.mobileno = mobileno;
		this.state = state;
		this.callnumber = callnumber;
		this.remark = remark;
		this.ctime = ctime;
		this.name = name;
		this.idcard = idcard;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getIdcard() {
		return idcard;
	}


	public void setIdcard(String idcard) {
		this.idcard = idcard;
	}


	public String getState() {
        return state;
    }

    public void setState(String state) {
        this.state = state;
    }

    public String getCallnumber() {
        return callnumber;
    }

    public void setCallnumber(String callnumber) {
        this.callnumber = callnumber;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getCustomerId() {
        return customerId;
    }

    public void setCustomerId(String customerId) {
        this.customerId = customerId;
    }

    public String getMobileno() {
        return mobileno;
    }

    public void setMobileno(String mobileno) {
        this.mobileno = mobileno;
    }

    public String getCtime() {
        return ctime;
    }

    public void setCtime(String ctime) {
        this.ctime = ctime;
    }


	public String getOperators() {
		return operators;
	}


	public void setOperators(String operators) {
		this.operators = operators;
	}


	@Override
	public String toString() {
		return "CustomerMobiles [id=" + id + ", customerId=" + customerId + ", mobileno=" + mobileno + ", state="
				+ state + ", callnumber=" + callnumber + ", remark=" + remark + ", ctime=" + ctime + ", name=" + name
				+ ", idcard=" + idcard + "]";
	}
    
}
