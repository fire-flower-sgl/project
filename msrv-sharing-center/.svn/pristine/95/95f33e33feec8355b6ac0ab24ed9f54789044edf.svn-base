	<select id="getServerList" resultMap="BaseResultMap">
		SELECT concat(id,'') id ,server_name from sp_server_admin ORDER BY server_name 
	</select>
	
	<!-- 根据服务名称模糊查询服务 -->
	<select id="findServerInfoByServerName" resultType="java.util.HashMap" >
	    select server_code as code ,server_name value
	    from sp_server_admin 
	    where 1=1 
	    <if test="serverName != null" >
	    	and server_name like '%${serverName}%'
	    </if>
	</select>
	
	<!-- 根据服务名称模糊查询服务id列表 -->
	<select id="findServerInfoByServerNameForId" resultType="java.util.HashMap" >
	    select CONCAT(id,'') as code ,server_name value
	    from sp_server_admin 
	    where 1=1 
	    <if test="serverName != null" >
	    	and server_name like '%${serverName}%'
	    </if>
	</select>
	
	<delete id="deleteByIds" parameterType="list" >
		delete from sp_server_admin
		where id in 
		<foreach collection="list" item="list" open="("  separator=","  close=")">
		  	#{list}
		</foreach> 
	</delete>
	
	
