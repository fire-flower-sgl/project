	<select id="findServerDetail" resultMap="DetailResultMap">
		select
			p.id, p.ip, p.server_name, p.server_code, p.username, p.email, p.mobile, 
			p.parent_server, p.status, p.created_time ,
			s.id sub_id, s.ip sub_ip, s.server_name sub_server_name, s.username sub_username, 
			s.email sub_email, s.mobile sub_email, s.parent_server sub_parent_server, 
			s.status, s.created_time sub_created_time
		from sp_server_admin p
			left join sp_server_admin s on s.server_code = p.parent_server
		where p.id = #{id}
	</select>
	
	<!-- 分页查询 -->
	<select id="listServerAdmin" parameterType="com.mhtech.platform.msrv.pojo.ServerPageInfo" resultMap="DetailResultMap">
		select
		<include refid="Detail_Column_List" />
		from sp_server_admin p
			left join sp_device_admin a on p.ip = a.ip
			left join sp_user b on b.user_code = p.username
		where 1 = 1
		<if test="serverName != null">
			and p.server_name like CONCAT('%', #{serverName,jdbcType=VARCHAR},'%')
		</if>
		<if test="username != null">
			and p.username like CONCAT('%', #{username,jdbcType=VARCHAR},'%')
		</if>
		<if test="serverType != null">
			and p.server_type = #{serverType,jdbcType=SMALLINT}
		</if>
		<if test="applyProjects != null">
			and p.apply_projects like CONCAT('%', #{applyProjects,jdbcType=VARCHAR},'%')
		</if>
		<if test="status != null">
			and p.status = #{status,jdbcType=SMALLINT}
		</if>
		<if test="monitorStatus != null">
			and a.status = #{monitorStatus,jdbcType=SMALLINT}
		</if>
	</select>
		
	<select id="findServerByServerId" parameterType="long" resultMap="BaseResultMap">
